.swiper-header{
  height: 300rpx;
}
.swiper-item{
  text-align: center;	
}
.box{
  width: 98%;
  height: 100%;
  display: inline-block;
  box-sizing: border-box;
  border-radius: 10px;
}
.swiper-item image{
  width: 100%;
  height: 100%;
  border-radius: 10px;
  border-image: 0;
}

.mvList-all{
  padding: 10rpx;
  box-sizing: border-box;
}
